{
	"TITLE": "Little-Bank",
	"PORT": 5000,
	"ISSUER": "https://appist.ir/",
	"SALT": 12,
	"CORS": {
		"origin": "http://localhost:3000",
		"methods": ["POST", "GET", "PUT", "PATCH", "DELETE"],
		"maxAge": 86400,
		"optionsSuccessStatus": 200
	},
	"avatar": {
		"path": {
			"user": "/little-bank/next/static/img/avatars/users/",
			"bank": "/little-bank/next/static/img/avatars/banks/",
			"post": "/little-bank/next/static/img/posts/",
			"export": "/little-bank/next/static/export/"
		},
		"cdn": {
			"user": "/img/avatars/users/",
			"bank": "/img/avatars/banks/",
			"post": "/img/posts/",
			"export": "/export/"
		}
	},
	"PASSPORT": {
		"session": false,
		"passReqToCallback": true
	}
}
